    padding: 0em 2em;
    display: grid;
    grid-template-columns: 1fr 1fr 1fr;
    grid-template-rows: 1fr 1fr;

}


.cookie-alert .cookie-alert--description {

    grid-column: 1 / 4;

}


.cookie-alert .cookie-alert--description #titulo {

